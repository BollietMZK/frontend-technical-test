{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "bracketSameLine": true,
  "proseWrap": "always",
  "singleAttributePerLine": false,
  "printWidth": 180,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.yml",
      "options": {
        "tabWidth": 2,
        "printWidth": 1500
      }
    }
  ]
}
